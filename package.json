{
  "name": "ros-quick-runner",
  "displayName": "ros-quick-runner",
  "description": "ros quick runner",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ros-launcher.roslaunch",
    "onCommand:ros-launcher.rosrun"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ros-launcher.roslaunch",
        "title": "ROSLaunch"
      },
      {
        "command": "ros-launcher.rosrun",
        "title": "ROSRun"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .launch",
          "command": "ros-launcher.roslaunch",
          "group": "navigation@1"
        },
        {
          "when": "resourceExtname == .py || resourceExtname == .cpp",
          "command": "ros-launcher.rosrun",
          "group": "navigation@1"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.16.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
